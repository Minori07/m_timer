<h1>Tasks#edit</h1>
<p>Find me in app/views/tasks/edit.html.erb</p>
<div style="width: 300px;">
  <%= form_for(@task) do |f| %>
    <div>
      <%= f.label :title %>
      <%= f.text_field :title, size: 30 %>
    </div>
    <div>
      <%= f.label :week %>
      <%= f.text_area :week %>
    </div>
    <div>
    	<%= f.label :time %>
      <%= f.text_area :time %>
    </div>
    <%= f.submit 'Submit' %>
  <% end %>
</div>
<p><%= link_to '一覧に戻る', tasks_url %></p>